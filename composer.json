{
    "name": "aeyoll/bootstrap-icons",
    "description": "Utility to get the markup of Bootstrap icons (https://icons.getbootstrap.com/)",
    "license": "MIT",
    "repositories": {
        "twbs-icons": {
            "type": "package",
            "package": {
                "name": "twbs/icons",
                "version": "1.9.0",
                "source": {
                    "url": "https://github.com/twbs/icons",
                    "type": "git",
                    "reference": "main"
                }
            }
        }
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "twbs/icons": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
